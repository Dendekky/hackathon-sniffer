{
  "name": "hackathon-sniffer-backend",
  "version": "0.1.0",
  "description": "Backend API for Hackathon Sniffer platform",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:seed": "tsx src/db/seed.ts",
    "ingest:once": "tsx src/ingestion/cli.ts once",
    "ingest:watch": "tsx src/ingestion/cli.ts watch",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix"
  },
  "keywords": ["hackathon", "api", "express", "typescript"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "sqlite3": "^5.1.6",
    "zod": "^3.22.4",
    "pino": "^8.16.1",
    "pino-pretty": "^10.2.3",
    "node-cron": "^3.0.3",
    "bottleneck": "^2.19.5",
    "cheerio": "^1.0.0-rc.12",
    "playwright": "^1.40.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.8.10",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.16",
    "tsx": "^4.1.2",
    "typescript": "^5.2.2"
  }
}
